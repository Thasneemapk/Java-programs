class Result {
    public static void main(String[] args) {
        int r1, r2, r3, r4;
        r1= -5 + 8 * 6;
        r2=(55+9) % 9;
        r3=20+-3*5/8;
        r4=5 + 15 / 3 * 2 - 8 % 3;

        System.out.println(r1) ;
        System.out.println(r2) ;
         System.out.println(r3); 
         System.out.println(r4);
    }
